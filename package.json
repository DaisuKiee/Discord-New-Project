{
  "name": "discord-bot-allinone",
  "version": "4.0.0",
  "description": "All-in-One Discord bot with AI, Music, Economy, Moderation & Web Dashboard",
  "main": "sharder.js",
  "type": "module",
  "scripts": {
    "start": "node sharder.js",
    "dev": "nodemon sharder.js",
    "dashboard": "node dashboard/server.js",
    "dev:dashboard": "nodemon dashboard/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:dashboard\"",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "commands:clear": "node scripts/clear-commands.js",
    "commands:refresh": "node scripts/refresh-commands.js"
  },
  "keywords": [
    "discord",
    "bot",
    "discord.js",
    "ai",
    "music",
    "dashboard"
  ],
  "author": "",
  "license": "GPL",
  "dependencies": {
    "discord.js": "^14.22.1",
    "dotenv": "^17.0.0",
    "mongoose": "^8.10.0",
    "signale": "^1.4.0",
    "@prisma/client": "^5.7.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "ejs": "^3.1.9",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "socket.io": "^4.6.1",
    "axios": "^1.6.2",
    "redis": "^4.6.11",
    "ioredis": "^5.3.2",
    "bull": "^4.12.0",
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "@google/generative-ai": "^0.1.3",
    "poru": "^5.0.2",
    "canvas": "^2.11.2",
    "chart.js": "^4.4.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "prisma": "^5.7.0"
  },
  "engines": {
    "node": ">=18"
  }
}
